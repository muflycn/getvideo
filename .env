# GetVideo 本地测试环境配置

# 应用基本配置
NODE_ENV=production
PORT=3000

# 文件存储路径
DOWNLOAD_PATH=/app/downloads
DATA_PATH=/app/data

# 下载配置
MAX_CONCURRENT_DOWNLOADS=3

# 安全配置 - 测试环境使用简单密钥
SESSION_SECRET=test-secret-key-for-local-development-only
BCRYPT_ROUNDS=12

# 日志配置
LOG_LEVEL=info

# 首次部署自动创建管理员账户
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Next.js 配置
NEXT_TELEMETRY_DISABLED=1

# 网络配置
HOSTNAME=0.0.0.0